    "    \"from domain.models.entities.bank_guarantee_metadata_entity import BankGuaranteeEntity\\n\",\n",
    "    \"def calculate_reduced_amount_from_values(origin_ccr:str,metadata:list[BankGuaranteeEntity])->float:\\n\",\n",
    "    \"     similar_ccr: list[\\n\",\n",
    "    \"            BankGuaranteeEntity] = _filter_metadata_by_origin_ccr(\\n\",\n",
    "    \"            metadata=metadata,\\n\",\n",
    "    \"            origin_ccr=origin_ccr,\\n\",\n",
    "    \"        )\\n\",\n",
    "    \"     total_ccr:float=_sum_reduced_from_values(similar_ccr)\\n\",\n",
